extends layout

block content
  script(src="https://code.jquery.com/jquery-2.2.4.min.js")
  script(src='https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.3/socket.io.js')
  script(src="/javascripts/main.js")
  div#container
    p#content Type "help" to navigate my e-portfolio<br> and landing-page to go back to the landing page<br>
    form#submitform
      div
        div#submittext B:/>
        input(type="text",name="command",rows="1",placeholder="Enter command",id="command",autofocus,autocorrect="off",autocomplete="off",autocapitalize="none")
      input#submitbutton(type="submit",tabindex=-1)

